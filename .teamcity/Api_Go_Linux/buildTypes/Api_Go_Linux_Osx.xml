<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="4eba7a4a-7533-4ea1-aca2-5775b171a851" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2021.1/project-config.xsd">
  <name>OSX</name>
  <description />
  <settings ref="Api_Go_Build">
    <parameters>
      <param name="env_path.sep" value=":" />
      <param name="platform" value="darwin-64bit" />
      <param name="update.cgo_cflags" value="export CGO_CFLAGS=&quot;$CGO_CFLAGS -I%QDB_API_PATH%/include&quot;" />
      <param name="update.cgo_ldflags" value="export CGO_LDFLAGS=&quot;$CGO_LDFLAGS -L`pwd`/%QDB_API_PATH%/lib -Wl,-rpath -Wl,`pwd`/%QDB_API_PATH%/lib&quot;" />
      <param name="update.library.path" value="export DYLD_LIBRARY_PATH=%QDB_API_PATH%/lib" spec="text validationMode='any'" />
    </parameters>
    <build-runners order="RUNNER_111, RUNNER_26, RUNNER_106, RUNNER_117, RUNNER_266, RUNNER_114, RUNNER_164">
      <runner id="RUNNER_106" name="Test" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[%echo.on%
%select.gcc%
%update.library.path%
%update.cgo_ldflags%
%update.cgo_cflags%
go test -v -test.run "Test*" -coverprofile=test-coverage.out]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_117" name="Examples" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[%echo.on%
%select.gcc%
%update.library.path%
%update.cgo_ldflags%
%update.cgo_cflags%
go test -v -test.run "Example*"]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
      <runner id="RUNNER_266" name="Documentation Examples" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[%echo.on%
%select.gcc%
%update.library.path%
%update.cgo_ldflags%
%update.cgo_cflags%
go run examples/tutorial/main.go]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <vcs-settings />
    <requirements>
      <equals id="RQ_1" name="teamcity.agent.jvm.os.name" value="Mac OS X" />
      <contains id="RQ_19" name="teamcity.agent.jvm.os.arch" value="x86_64" />
    </requirements>
    <build-triggers />
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_1" sourceBuildTypeId="Quasardb_Artifacts_Release_MacOS_X8664Core2_Composite" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="qdb-*-c-api.tar.zst!**/* =&gt; qdb&#xD;&#xA;qdb-*-utils.tar.zst!**/* =&gt; qdb&#xD;&#xA;qdb-*-server.tar.zst!**/* =&gt; qdb" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="Quasardb_Artifacts_Release_MacOS_X8664Core2_Composite">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
    <cleanup />
  </settings>
</build-type>

