<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="60149332-23f8-4155-8a8c-7d35663a6c79" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2025.3/project-config.xsd">
  <name>x86_64 Core2</name>
  <description />
  <settings ref="Api_Go_LinuxBuild">
    <build-runners>
      <runner id="RUNNER_61" name="Generate documentation" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[go get -v golang.org/x/tools/cmd/godoc

rm -rf ./doc/*.html

godoc -http=localhost:6060 &
DOC_PID=$!

sleep 5

curl -o doc/index.html http://localhost:6060/pkg/github.com/bureau14/qdb-api-go/

kill -9 $DOC_PID

tar -czvf doc.tar.gz doc/*]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <requirements>
      <equals id="RQ_1" name="teamcity.agent.jvm.os.name" value="Linux" />
      <equals id="RQ_2" name="teamcity.agent.jvm.os.arch" value="amd64" />
    </requirements>
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_1" sourceBuildTypeId="Quasardb_Artifacts_Release_Linux_X8664Core2_Composite" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="qdb-*-c-api.tar.zst!**/* =&gt; qdb&#xD;&#xA;qdb-*-utils.tar.zst!**/* =&gt; qdb&#xD;&#xA;qdb-*-server.tar.zst!**/* =&gt; qdb" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="Quasardb_Artifacts_Release_Linux_X8664Core2_Composite">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
  </settings>
</build-type>

