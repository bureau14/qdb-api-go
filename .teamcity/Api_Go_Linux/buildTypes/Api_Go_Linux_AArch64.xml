<?xml version="1.0" encoding="UTF-8"?>
<build-type xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" uuid="d54bec4d-9325-406e-b0cf-58737811f302" xsi:noNamespaceSchemaLocation="https://www.jetbrains.com/teamcity/schemas/2025.3/project-config.xsd">
  <name>AArch64</name>
  <description />
  <settings ref="Api_Go_LinuxBuild">
    <parameters>
      <param name="goroot" value="%goroot%" />
    </parameters>
    <build-runners>
      <runner id="RUNNER_61" name="Generate documentation" type="simpleRunner">
        <parameters>
          <param name="script.content"><![CDATA[go get -v golang.org/x/tools/cmd/godoc

rm -rf ./doc/*.html

godoc -http=localhost:6060 &
DOC_PID=$!

sleep 5

curl -o doc/index.html http://localhost:6060/pkg/github.com/bureau14/qdb-api-go/

kill -9 $DOC_PID

tar -czvf doc.tar.gz doc/*]]></param>
          <param name="teamcity.step.mode" value="default" />
          <param name="use.custom.script" value="true" />
        </parameters>
      </runner>
    </build-runners>
    <requirements>
      <equals id="RQ_1" name="teamcity.agent.jvm.os.name" value="Linux" />
      <equals id="RQ_2" name="teamcity.agent.jvm.os.arch" value="aarch64" />
    </requirements>
    <build-extensions>
      <extension id="matrix" type="matrix">
        <parameters>
          <param name="matrix.param.goroot.1" value="goroot" />
          <param name="matrix.value.goroot.1" value="%system.go1.23-64.goroot%" />
          <param name="matrix.value.goroot.2" value="%system.go1.24-64.goroot%" />
        </parameters>
      </extension>
    </build-extensions>
    <artifact-dependencies>
      <dependency id="ARTIFACT_DEPENDENCY_1" sourceBuildTypeId="Quasardb_Artifacts_Release_Linux_AArch64_Composite" cleanDestination="false">
        <revisionRule name="sameChainOrLastFinished" revision="latest.sameChainOrLastFinished" />
        <artifact sourcePath="qdb-*-c-api.tar.zst!**/* =&gt; qdb&#xD;&#xA;qdb-*-utils.tar.zst!**/* =&gt; qdb&#xD;&#xA;qdb-*-server.tar.zst!**/* =&gt; qdb" />
      </dependency>
    </artifact-dependencies>
    <dependencies>
      <depend-on sourceBuildTypeId="Quasardb_Artifacts_Release_Linux_AArch64_Composite">
        <options>
          <option name="take-started-build-with-same-revisions" value="true" />
          <option name="take-successful-builds-only" value="true" />
        </options>
      </depend-on>
    </dependencies>
  </settings>
</build-type>

